#!/usr/bin/env python
class basic:
    def __init__(self,x):
        self.x = x

    def prime(self):
        prime1 = 0
        for i in range(2,int(self.x)):
           if int(self.x) % i == 0 :
             prime1 = 1
        return prime1
    def fib(self):
        a = 0
        b = 1
        lst = []
        for i in range(1,int(self.x)):
            lst.append(b)
            c = a
            a = b
            b = b+c
        return lst
    def reverse(self):
        new = ''
        for i in str(self.x):
            new = i + new
        return new
    def factorial(self):
        fact = 1
        for i in range(1,int(self.x+1)):
            fact = fact * i
        return fact    
        
